<body>
    <div class="top-of-page">
        <div class="row">
            <div class="col-9">
                <h1 style="color:white">{{name}}</h1> 
            </div>
            <div class="col-3">
                <a href = "/edit-assignment-from-start"> <img src="/images/edit project.png" style="width:10vw;margin-top:2vw;"></a>
            </div>
         </div>
     </div>
 <div class ="container">
    <div class ="text-center">
        <center>
            </br>
            <h2>Timer Until Start:</h2>
             <div id="progress-timer"></div>
            </br>
           
            {{#if viewAlt}}
            <a href="/progress-checker"><button type="button" class="btn btn-danger btn-wide btn-block" id="starEarly-button" style="width:70vw; background-color: #009688; border-color: #009688;" align="middle"> Start Now! </button></a>

            {{else}}
            <a href="/progress-checker"><button type="button" class="btn btn-danger btn-wide btn-block" id="starEarly-button" style="width:70vw; background-color: #009688; border-color: #009688;" align="middle"> Start Early! </button></a>
            {{/if}}


        </center>
    </div>


    <div>
        </br>
        </br>
        <button id="myBtn">See what happens when countdown hits 0</button>
        </br>
        </br>   
        <button id="myBtnAGAIN">See what happens When countdown hits 0 and user has delayed more than once</button>
         <!--<button id ="myBtn" onclick="document.getElementById('sound1').play();">When countdown hits 0</button> -->
    </div>

    <div id="myModal" class="modal">
        <div class="modal-content">
            <!--<span class="close">&times;</span>-->            
            <div class="startOptions">
                <h1>It's time to start working on the assignment</h1>
            </div>

            <div class="startOptions">
                <a href="/progress-checker">Start!</a>
                <div class="btnDivider" />
                <a href="/delay-start.html">Delay</a>
            </div>
    </div>

    <div id="myModalAGAIN" class="modal">
        <div class="modal-content">
            <!--<span class="close">&times;</span>-->        
            <div class="startOptions">
                <h1>IT'S TIME TO START ON YOUR ASSIGNMENT NOW!!!!</h1>
            </div>           

        <div class="startOptions">
            <a href="/progress-checker">START NOW!</a>
            <div class="btnDivider" />
            <a href="/delay-start.html">DELAY AGAIN</a>
            <!--<a href="delay-startAGAIN.html">DELAY AGAIN</a> CREATE THIS PAGE-->
        </div>
    </div>

